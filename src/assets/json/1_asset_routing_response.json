    "host": "tu-connector.gaiax.cluster.tsachweh.de",
    "version": "1.0"
  },
  "dataAddress": {
    "type": "Kafka",
    "kafka.bootstrap.servers": "kafka-service.gaiax.svc.cluster.local:9092",
    "topic": "tu-routing-response"
  }
}
